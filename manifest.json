{
  "name": "Anon PR",
  "version": "1.0",
  "description": "Anonymize PR reviews",
  "manifest_version": 2,
  "permissions": [
    "https://bitbucket.org/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://bitbucket.org/dashboard/overview",
        "https://bitbucket.org/*/pull-requests/*"
      ],
      "css": [
        "bitbucket.css"
      ]
    }
  ]
}
